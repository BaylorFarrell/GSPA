{
  "manifest_version": 2,
  "name": "Google Search Parameter Appender",
  "version": "1.1",
  "description": "Appends custom parameters to Google search URLs.",
  "applications": {
    "gecko": {
      "id": "{33fbba68-20cd-4392-9952-a066653890a7}"
    }
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://www.google.com/search*",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Set search parameters"
  }
}